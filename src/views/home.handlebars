<h1>Lista de Productos:</h1>
<div class="container" id="productsFeed">
    {{#each products.docs}}
    <div class="div-card">
        <div class="title">
            <p>{{title}}</p>
        </div>
        <div class="image">
            <p>{{thumbnail}}</p>
        </div>
        <div class="description">
            <p>Descripcion: {{description}}</p>
        </div>
        <div class="price">
            <p>Precio: ${{price}}</p>
        </div>
        <div class="code-stock">
            <p>Codigo: {{code}}</p>
            <p>Stock: {{stock}}</p>
        </div>
    </div>
    {{/each}}

</div>
<div>
    Current page: {{products.page}}
    {{#if products.hasPrevPage}}
    <a href="/?page={{products.prevPage}}">Prev</a>
    {{/if}}

    {{#if products.hasNextPage}}
    <a href="/?page={{products.nextPage}}">Next</a>
    {{/if}}

</div>