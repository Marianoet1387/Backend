<div>
    <form action="/realTimeProducts" id="Form" method="post" enctype="application/x-www-form-urlencoded">
        <div>
            <p>Title</p>
            <input type="text" id="Name" name="title">

            <p>Description</p>
            <input type="text" id="Description" :name="description">

            <p>Price</p>
            <input type="number" id="Number" name="price">

            <p>Image</p>
            <input type="text" id="thumbnail" name="thumbnail">

            <p>Stock</p>
            <input type="text" id="Stock" name="stock">

            <p>Code</p>
            <input type="text" id="Code" name="code">

            <input type="submit" id="addProduct" value="Agregar">
        </div>
    </form>
</div>

<div id="productsFeed">
    <h3>Productos:</h3>
    <ul>
        {{#each products}}
        <li> title: {{title}},description: {{description}}, price: {{price}},thumbnail: {{thumbnail}}, title: {{code}},stock: {{stock}} </li>
        {{/each}}
    </ul>
</div>